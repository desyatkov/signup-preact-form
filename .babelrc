{
  "presets": ["preact-widget-scripts/babel"],
  "plugins": [
    [
      "@babel/plugin-transform-regenerator",
      {
        "asyncGenerators": true,
        "generators": true,
        "async": true
      }
    ],
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
      }
    }]
  ]
}
